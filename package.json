{
  "name": "whitespace-tools",
  "version": "1.0.0",
  "engines": {
    "node": ">=8.0"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "description": "Line ending and beginning-of-line whitespace space correction tools",
  "repository": "https://github.com/jlyonsmith/whitespace-tools.git",
  "files": [
    "dist/**"
  ],
  "bin": {
    "ender": "dist/ender-cli.js",
    "spacer": "dist/spacer-cli.js"
  },
  "scripts": {
    "build": "babel src -d dist -s --ignore *.test.js",
    "debug:ender": "babel-node --inspect-brk src/ender-cli.js",
    "debug:spacer": "babel-node --inspect-brk src/spacer-cli.js",
    "test": "jest"
  },
  "keywords": [
    "end",
    "space",
    "code"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "dependencies": {
    "auto-bind2": "^1.0.3",
    "concat-stream": "^1.6.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "istanbul": "^0.4.5",
    "jest": "^21.1.0",
    "jest-babel-istanbul": "^0.1.0",
    "nodemon": "^1.12.1",
    "tmp": "0.0.33"
  }
}
